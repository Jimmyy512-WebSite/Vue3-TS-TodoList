    "element-plus": "^2.11.5",
    "intro.js": "^8.3.2",
    "json-server": "^0.17.4",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3"
