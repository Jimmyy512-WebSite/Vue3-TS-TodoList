    "element-plus": "^2.7.8",
    "intro.js": "^7.2.0",
    "json-server": "^0.17.4",
    "normalize.css": "^8.0.1",
    "pinia": "^2.2.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "vue": "^3.4.35",
    "vue-i18n": "^9.13.1",
