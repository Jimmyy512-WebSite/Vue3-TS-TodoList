{
  "name": "my-vue-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run json-server\" \"vite\"",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "json-server": "./node_modules/.bin/json-server  db.json --port 3087",
    "linux-server": "pm2 start /root/Vue3-TS-TodoList/node_modules/.bin/json-server -- /root/Vue3-TS-TodoList/db.json --port 3087"
  },
  "dependencies": {
    "@types/node": "^17.0.8",
    "autoprefixer": "^10.4.1",
    "axios": "^0.24.0",
    "element-plus": "^1.3.0-beta.3",
    "intro.js": "^4.3.0",
    "json-server": "^0.17.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.9",
    "postcss": "^8.4.5",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "concurrently": "^7.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.1",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "less": "^4.1.2",
    "postcss-less": "^5.0.0",
    "sass": "^1.47.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.8"
  }
}
